{
  "ConnectionStrings": {
    "DefaultConnection": "Server=localhost;Port=5432;Database=MVC_MovieProject;User Id=postgres;Password=;",
    "ApplicationDbContextConnection": "Server=(localdb)\\mssqllocaldb;Database=MovieProject;Trusted_Connection=True;MultipleActiveResultSets=true"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "AppSettings": {
    "MovieProjectSettings": {
      "DefaultBackdropSize": "original",
      "DefaultPosterSize": "w500",
      "DefaultCastImage": "",
      "DefaultCollection": {
        "Name": "All",
        "Description": "All imported movies will automatically be assigned to the 'All' collection."
      }
    },
    "TmDBSettings": {
      "BaseUrl": "https://api.themoviedb.org/3",
      "BaseImagePath": "https://image.tmdb.org/t/p",
      "BaseYouTubePath": "//www.youtube.com/watch?v=",
      "QueryOptions": {
        "Language": "en-US",
        "AppendToResponse": "videos,release_dates,credits",
        "Page": "1"
      }
    }
  }
}